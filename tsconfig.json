{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2023",                              
    "rootDir": "./",
    "outDir": "./dist",  
    "moduleResolution": "bundler", 
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "5.0"
  },
  "include": ["src", "prisma.config.ts"],
    "exclude": ["node_modules", "dist"]
}